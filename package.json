{
	"name": "@everipedia/iq-login",
	"version": "7.1.4",
	"description": "Auth library for IQ apps",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./client": {
			"import": "./dist/client.js",
			"require": "./dist/client.cjs",
			"types": "./dist/client.d.ts"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"repository": "https://github.com/EveripediaNetwork/iq-login",
	"license": "MIT",
	"scripts": {
		"format": "biome format . --write",
		"lint": "biome check .",
		"lint:fix": "pnpm lint --write",
		"build": "tsup",
		"watch:build": "tsup --watch",
		"release": "pnpm build && changeset publish"
	},
	"engines": {
		"node": ">=10"
	},
	"dependencies": {
		"@changesets/cli": "^2.29.2",
		"@everipedia/web3-signer": "^0.0.13",
		"@wagmi/core": "^2.16.7",
		"@web3auth/base": "^9.7.0",
		"@web3auth/ethereum-provider": "^9.7.0",
		"@web3auth/modal": "^9.7.0",
		"@web3auth/wallet-services-plugin": "^9.7.0",
		"@web3auth/web3auth-wagmi-connector": "^7.0.0",
		"boring-avatars": "^1.11.2",
		"cookies-next": "^5.1.0",
		"lucide-react": "^0.488.0",
		"vitest": "^3.1.1",
		"zod": "^3.24.3",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@tanstack/react-query": "^5.74.3",
		"@types/node": "^22.14.1",
		"@types/react": "^19.1.2",
		"changeset": "^0.2.6",
		"react": "^19.1.0",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3",
		"viem": "~2.27.2",
		"wagmi": "^2.14.16"
	},
	"peerDependencies": {
		"@tanstack/react-query": "^5.53.1",
		"next": "15.0.0",
		"react": "19.0.0-rc-65a56d0e-20241020",
		"react-dom": "19.0.0-rc-65a56d0e-20241020",
		"viem": "2.x",
		"wagmi": "^2.12.4"
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.json",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	}
}
